{
  "name": "sigma-file-manager",
  "version": "2.0.0-beta.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.13.1"
  },
  "volta": {
    "node": "22.13.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "check": "run-p type-check lint:check",
    "type-check": "vue-tsc --build",
    "lint:eslint": "eslint . --fix",
    "lint:stylelint": "stylelint \"src/**/*.{css,vue}\" --fix",
    "lint:eslint:check": "eslint . --max-warnings 0",
    "lint:stylelint:check": "stylelint \"src/**/*.{css,vue}\" --max-warnings 0",
    "lint:check": "run-s lint:*:check",
    "lint": "run-s lint:eslint lint:stylelint",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:dev:webkit-igpu": "env WEBKIT_DISABLE_COMPOSITING_MODE=1 npm run tauri dev",
    "tauri:build": "tauri build",
    "tauri:gen-icons": "tauri icon ./src/assets/icons/logo-1024x1024.png",
    "tauri:cargo-update": "cd ./src-tauri && cargo update",
    "tauri:cargo-build": "cd ./src-tauri && cargo build",
    "sync-i18n": "npx tsx ./scripts/sync-i18n.js",
    "sync-license": "npx tsx ./scripts/sync-license.js"
  },
  "dependencies": {
    "@crabnebula/tauri-plugin-drag": "^2.1.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tauri-apps/api": "^2.6.0",
    "@tauri-apps/plugin-dialog": "^2.5.0",
    "@tauri-apps/plugin-opener": "^2.5.0",
    "@tauri-apps/plugin-os": "^2.3.2",
    "@tauri-apps/plugin-store": "^2.3.0",
    "@vueuse/core": "^13.9.0",
    "class-variance-authority": "^0.7.1",
    "dayjs": "^1.11.13",
    "lodash.clonedeep": "^4.5.0",
    "lucide-vue-next": "^0.525.0",
    "minimatch": "^10.1.1",
    "pinia": "^3.0.3",
    "reka-ui": "^2.7.0",
    "tailwindcss": "^4.1.11",
    "tauri-plugin-system-fonts-api": "^2.0.2",
    "v-wave": "^3.0.3",
    "vaul-vue": "^0.4.1",
    "vue": "^3.5.17",
    "vue-i18n": "^11.1.9",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.9",
    "vue3-smooth-dnd": "^0.0.6"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^4.1.0",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@tauri-apps/cli": "^2.6.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/node": "^24.0.10",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.30.1",
    "eslint-plugin-better-tailwindcss": "^3.4.4",
    "eslint-plugin-check-file": "^3.3.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "readdirp": "^4.1.2",
    "stylelint": "^16.21.1",
    "stylelint-config-css-modules": "^4.5.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "typescript": "~5.8.3",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-run": "^0.6.1",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.1"
  }
}